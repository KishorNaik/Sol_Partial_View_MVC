@model Sol_Partial_View.ViewModel.UserViewModel
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<div class="row">
    <div class="col-12">
        <div class="form-group">
            <input id="txtUserName" type="text" asp-for="Users.UsersLogin.UserName" class="form-control" placeholder="User Name" />
        </div>
    </div>
</div>
<div class="row">
    <div class="col-12">
        <div class="form-group">
            <input id="txtPassword" type="password" asp-for="Users.UsersLogin.Password" class="form-control" placeholder="Password" />
        </div>
    </div>
</div>
<div class="row">
    <div class="col-12">
        <div class="form-group">
            <input id="btnSubmit" type="submit" value="Submit" class="btn btn-primary" />
        </div>
    </div>
</div>


